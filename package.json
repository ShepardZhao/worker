    "jasmine-core": "*",
    "karma": "*",
    "karma-coverage": "*",
    "karma-jasmine": "*",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "*",
    "phantomjs-prebuilt": "^2.1.7",
    "uglifyjs": "*"
  }
